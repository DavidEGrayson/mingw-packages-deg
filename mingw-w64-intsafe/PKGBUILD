# Maintainer: David Grayson <davidegrayson@gmail.com>

_realname=intsafe
pkgbase=mingw-w64-${_realname}
pkgname="${MINGW_PACKAGE_PREFIX}-${_realname}"
pkgver=1
pkgrel=2
pkgdesc="Implementation of Microsoft's intsafe.h header (mingw-w64)"
arch=('any')
url='https://github.com/DavidEGrayson/intsafe'
license=('custom')
source=(
  'https://raw.githubusercontent.com/DavidEGrayson/intsafe/master/generated/intsafe.h'
)

sha256sums=(
  'SKIP'
)

package() {
  cd "${srcdir}"
  mkdir -p "${pkgdir}${MINGW_PREFIX}/include/"
  install -m644 intsafe.h "${pkgdir}${MINGW_PREFIX}/include/"
}
